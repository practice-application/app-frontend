{
  "name": "application",
  "version": "1.0.0",
  "description": "Frontend of applicaition",
  "private": true,
  "dependencies": {
    "@auth0/auth0-react": "^1.8.0",
    "@babel/core": "^7.16.0",
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.5",
    "@mui/lab": "^5.0.0-alpha.53",
    "@mui/material": "^5.0.6",
    "axe-core": "^4.3.5",
    "date-fns": "^2.25.0",
    "firebase": "^9.3.0",
    "lodash.clonedeep": "^4.5.0",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-moment": "^1.1.1",
    "react-router-dom": "^6.0.0",
    "react-router-hash-link": "^2.4.3",
    "react-scripts": "^3.4.1",
    "react-select-country-list": "^2.2.3",
    "typescript": "^3.9.5",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "REACT_APP_STAGE=dev react-scripts start",
    "build": "react-scripts build",
    "build:stg": "REACT_APP_STAGE=stg react-scripts build",
    "build:prd": "REACT_APP_STAGE=prd react-scripts build",
    "test": "react-scripts test",
    "e2e": "CYPRESS_BASE_URL=http://localhost:3000 cypress run --headless",
    "e2e:open": "CYPRESS_BASE_URL=http://localhost:3000 cypress open"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:jsx-a11y/recommended",
      "plugin:cypress/recommended"
    ],
    "plugins": [
      "jsx-a11y",
      "import"
    ],
    "rules": {
      "import/order": [
        "warn",
        {
          "groups": [
            "builtin",
            "external",
            "internal"
          ],
          "pathGroups": [
            {
              "pattern": "react",
              "group": "external",
              "position": "before"
            }
          ],
          "pathGroupsExcludedImportTypes": [
            "react"
          ],
          "newlines-between": "always",
          "alphabetize": {
            "order": "asc",
            "caseInsensitive": true
          }
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "cypress": "^6.5.0",
    "cypress-axe": "^0.13.0",
    "eslint": "^6.8.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "prop-types": "^15.7.2"
  }
}
