{
  "name": "application",
  "version": "1.0.0",
  "description": "Frontend of applicaition",
  "private": true,
  "dependencies": {
    "@auth0/auth0-react": "^1.7.0",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.0-rc.0",
    "@mui/lab": "^5.0.0-alpha.48",
    "@mui/material": "^5.0.0-rc.0",
    "@mui/system": "^5.0.6",
    "date-fns": "^2.23.0",
    "firebase": "8.3.1",
    "lodash.clonedeep": "^4.5.0",
    "moment": "^2.29.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-images-uploading": "^3.1.3",
    "react-moment": "^1.1.1",
    "react-router-dom": "^5.3.0",
    "react-router-hash-link": "^1.2.2",
    "react-scripts": "^3.4.1",
    "react-select-country-list": "^2.2.3",
    "typescript": "^3.9.5",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "REACT_APP_STAGE=dev react-scripts start",
    "build": "react-scripts build",
    "build:stg": "REACT_APP_STAGE=stg react-scripts build",
    "build:prd": "REACT_APP_STAGE=prd react-scripts build",
    "test": "react-scripts test",
    "e2e": "CYPRESS_BASE_URL=http://localhost:3000 cypress run --headless",
    "e2e:open": "CYPRESS_BASE_URL=http://localhost:3000 cypress open"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:jsx-a11y/recommended",
      "plugin:cypress/recommended"
    ],
    "plugins": [
      "jsx-a11y",
      "import"
    ],
    "rules": {
      "import/order": [
        "warn",
        {
          "groups": [
            "builtin",
            "external",
            "internal"
          ],
          "pathGroups": [
            {
              "pattern": "react",
              "group": "external",
              "position": "before"
            }
          ],
          "pathGroupsExcludedImportTypes": [
            "react"
          ],
          "newlines-between": "always",
          "alphabetize": {
            "order": "asc",
            "caseInsensitive": true
          }
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "cypress": "^6.5.0",
    "cypress-axe": "^0.8.1",
    "eslint": "^6.8.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "prop-types": "^15.6.2"
  }
}
